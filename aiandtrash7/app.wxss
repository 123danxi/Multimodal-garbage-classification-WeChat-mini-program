page {
  width: 100vw;
  height: 100vh;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Cantarell', 'Fira Sans',
    'Droid Sans', 'Helvetica Neue', 'Microsoft Yahei', sans-serif;
}

view,
image,
text {
  box-sizing: border-box;
  flex-shrink: 0;
}

.codef-flex-row {
  display: flex;
  flex-direction: row;
}

.codef-flex-col {
  display: flex;
  flex-direction: column;
}

.codef-justify-start {
  justify-content: flex-start;
}

.codef-justify-end {
  justify-content: flex-end;
}

.codef-justify-center {
  justify-content: center;
}

.codef-justify-between {
  justify-content: space-between;
}

.codef-justify-around {
  justify-content: space-around;
}

.codef-justify-evenly {
  justify-content: space-evenly;
}

.codef-items-start {
  align-items: flex-start;
}

.codef-items-end {
  align-items: flex-end;
}

.codef-items-center {
  align-items: center;
}

.codef-items-baseline {
  align-items: baseline;
}

.codef-items-stretch {
  align-items: stretch;
}

.codef-self-start {
  align-self: flex-start;
}

.codef-self-end {
  align-self: flex-end;
}

.codef-self-center {
  align-self: center;
}

.codef-self-baseline {
  align-self: baseline;
}

.codef-self-stretch {
  align-self: stretch;
}

.codef-flex-1 {
  flex: 1 1 0%;
}

.codef-flex-auto {
  flex: 1 1 auto;
}

.codef-grow {
  flex-grow: 1;
}

.codef-grow-0 {
  flex-grow: 0;
}

.codef-shrink {
  flex-shrink: 1;
}

.codef-shrink-0 {
  flex-shrink: 0;
}

.codef-relative {
  position: relative;
}

